{
    "author":"Techschneiderrr",
    "manifest_version": 2,
    "name": "DA-Dates",
    "version": "0.0.0",
    "description": "Checkout the last dates of da's and also download them",
    "content_scripts": [
        {
            "run_at":"document_start",
            "js": [
                "content.js"
            ],
            "matches": [
                "https://vtop.vit.ac.in/vtop/initialProcess"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://vtop.vit.ac.in/vtop/*"
    ]
}